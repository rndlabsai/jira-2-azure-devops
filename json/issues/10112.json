  "key": "GG-109",
  "fields": {
    "summary": "Document Migration Process with Focus on Security and Sequential Steps",
    "description": {
      "type": "doc",
      "version": 1,
      "content": [
        {
          "type": "heading",
          "attrs": {
            "level": 2
          },
          "content": [
            {
              "type": "text",
              "text": "Use case:"
            }
          ]
        },
        {
          "type": "paragraph",
          "content": [
            {
              "type": "text",
              "text": "As a "
            },
            {
              "type": "text",
              "text": "Product Owner",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "hardBreak"
            },
            {
              "type": "text",
              "text": "I want to "
            },
            {
              "type": "text",
              "text": "document the migration process",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": " with a focus on security and sequential steps, "
            },
            {
              "type": "hardBreak"
            },
            {
              "type": "text",
              "text": "so that "
            },
            {
              "type": "text",
              "text": "the development team can understand the requirements and build the migration process accordingly",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": "."
            }
          ]
        },
        {
          "type": "rule"
        },
        {
          "type": "heading",
          "attrs": {
            "level": 2
          },
          "content": [
            {
              "type": "text",
              "text": "Acceptance criteria:"
            }
          ]
        },
        {
          "type": "paragraph",
          "content": [
            {
              "type": "text",
              "text": "GIVEN",
              "marks": [
                {
                  "type": "em"
                }
              ]
            },
            {
              "type": "text",
              "text": " "
            },
            {
              "type": "text",
              "text": "the need to migrate Jira Cloud data to Azure DevOps",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "hardBreak"
            },
            {
              "type": "text",
              "text": "WHEN",
              "marks": [
                {
                  "type": "em"
                }
              ]
            },
            {
              "type": "text",
              "text": " "
            },
            {
              "type": "text",
              "text": "documenting the migration process",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "hardBreak"
            },
            {
              "type": "text",
              "text": "AND",
              "marks": [
                {
                  "type": "em"
                }
              ]
            },
            {
              "type": "text",
              "text": " "
            },
            {
              "type": "text",
              "text": "focusing on security measures and sequential steps",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": ", "
            },
            {
              "type": "hardBreak"
            },
            {
              "type": "text",
              "text": "THEN",
              "marks": [
                {
                  "type": "em"
                }
              ]
            },
            {
              "type": "text",
              "text": " "
            },
            {
              "type": "text",
              "text": "the document should cover all the necessary steps for secure and sequential migration",
              "marks": [
                {
                  "type": "strong"
                }
              ]
            },
            {
              "type": "text",
              "text": ". "
            }
          ]
        },
        {
          "type": "rule"
        },
        {
          "type": "heading",
          "attrs": {
            "level": 2
          },
          "content": [
            {
              "type": "text",
              "text": "Technical notes:"
            }
          ]
        },
        {
          "type": "paragraph",
          "content": [
            {
              "type": "text",
              "text": "The document should outline the following:"
            }
          ]
        },
        {
          "type": "bulletList",
          "content": [
            {
              "type": "listItem",
              "content": [
                {
                  "type": "paragraph",
                  "content": [
                    {
                      "type": "text",
                      "text": "Detailed steps for securely handling credentials throughout the migration process."
                    }
                  ]
                }
              ]
            },
            {
              "type": "listItem",
              "content": [
                {
                  "type": "paragraph",
                  "content": [
                    {
                      "type": "text",
                      "text": "Description of sequential checkpoints for ensuring a step-by-step migration of Jira Cloud data to Azure DevOps."
                    }
                  ]
                }
              ]
            },
            {
              "type": "listItem",
              "content": [
                {
                  "type": "paragraph",
                  "content": [
                    {
                      "type": "text",
                      "text": "Clear instructions for storing and sharing exported JSON data securely with other teams involved in the migration process."
                    }
                  ]
                }
              ]
            },
            {
              "type": "listItem",
              "content": [
                {
                  "type": "paragraph",
                  "content": [
                    {
                      "type": "text",
                      "text": "Guidelines for excluding sensitive data, such as Zephyr test artifacts, from the exported JSON."
                    }
                  ]
                }
              ]
            },
            {
              "type": "listItem",
              "content": [
                {
                  "type": "paragraph",
                  "content": [
                    {
                      "type": "text",
                      "text": "Recommendations for implementing security best practices, including secure API token management and avoiding exposure of credentials in logs or version control."
                    }
                  ]
                }
              ]
            }
          ]
        }
      ]
    },
